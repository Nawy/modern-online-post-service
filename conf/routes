
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~



GET            /users/:id         controllers.UserController.get(id:String)
POST           /users/save        controllers.UserController.save
POST           /users             controllers.UserController.insert
DELETE         /users/:id         controllers.UserController.delete(id:String)

POST           /messages          controllers.MessageController.insert


      